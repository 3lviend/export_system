<h1>Listing catches</h1>

<table>
  <tr>
    <th>Fish</th>
    <th>Length</th>
    <th>Weight</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @catches.each do |catch| %>
  <tr>
    <td><%= catch.fish_id %></td>
    <td><%= catch.length %></td>
    <td><%= catch.weight %></td>
    <td><%= link_to 'Show', catch %></td>
    <td><%= link_to 'Edit', edit_catch_path(catch) %></td>
    <td><%= link_to 'Destroy', catch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Catch', new_catch_path %>
