<h1>Listing fish</h1>

<table>
  <tr>
    <th>Family</th>
    <th>Genus</th>
    <th>Species</th>
    <th>English name</th>
    <th>Local name</th>
    <th>Code</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @fish.each do |fish| %>
  <tr>
    <td><%= fish.family %></td>
    <td><%= fish.genus %></td>
    <td><%= fish.species %></td>
    <td><%= fish.english_name %></td>
    <td><%= fish.local_name %></td>
    <td><%= fish.code %></td>
    <td><%= link_to 'Show', fish %></td>
    <td><%= link_to 'Edit', edit_fish_path(fish) %></td>
    <td><%= link_to 'Destroy', fish, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Fish', new_fish_path %>
